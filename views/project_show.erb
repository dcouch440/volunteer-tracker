<div id="list">
  <%=
      <<-HERE
        <p>
          #{@project.title}
        </p>
      HERE
  %>
</div>

<a href="/projects/<%=@project.id%>/edit">Edit Project</a>

<a href="/projects/<%=@project.id%>/volunteers/new">Add a volunteer?</a>

<div>
  <%=
    @volunteers.map {|volunteer|
      <<-HERE
        <p>
          <a href="/projects/#{volunteer.project_id}/volunteers/#{volunteer.id}"
          >#{volunteer.name}</a>
        </p>
      HERE
    }.map(&:strip).join('')
  %>
</div>